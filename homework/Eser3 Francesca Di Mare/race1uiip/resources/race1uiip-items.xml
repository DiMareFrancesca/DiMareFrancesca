<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">
	<!-- enumtype per l'attributo retired (che può essere Yes o no) di StageRanking-->		
			<enumtypes>
		<enumtype code="retiredEnum" autocreate="true" generate="true"
			dynamic="true">
			<value code="YES" />
			<value code="NO" />
		</enumtype>
	</enumtypes>

	<relations>
	<!-- relazione 1-n tra StageRace e Cyclist -->
	<relation localized="false" code="StageRace2CyclistRelation">
		<sourceElement type="StageRace" qualifier="stageRace"
			cardinality="one">
			<modifiers optional="true" />
		</sourceElement>
		<targetElement type="Cyclist" qualifier="cyclist"
			cardinality="many">
			<modifiers optional="true" />
		</targetElement>
	</relation>

	<!-- relazione 1-n tra StageRace e Stage -->
	<relation localized="false" code="StageRace2StageRelation">
		<sourceElement type="StageRace" qualifier="stageRace"
			cardinality="one">
			<modifiers optional="true" />
		</sourceElement>
		<targetElement type="Stage" qualifier="stage"
			cardinality="many">
			<modifiers optional="true" />
		</targetElement>
	</relation>

	<!-- relazione n-n tra Stage e Cyclist -->
	<relation localized="false" code="Stage2CyclistRelation">
		<deployment table="Stage2Cyclistrelation" typecode="15124" />
		<sourceElement type="Stage" qualifier="stage"
			cardinality="many">
			<modifiers optional="true" />
		</sourceElement>
		<targetElement type="Cyclist" qualifier="cyclist"
			cardinality="many">
			<modifiers optional="true" />
		</targetElement>
	</relation>
</relations>

	<!-- itemtype of Cyclist table -->
	<itemtypes>
		<itemtype code="Cyclist" autocreate="true" generate="true">
			<deployment table="Cyclist" typecode="15888" />
			<attributes>
				<attribute qualifier="id" type="java.lang.String">
					<modifiers optional="false" unique="true" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="name" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="stageRanking" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>

			</attributes>
		</itemtype>




		<!-- itemtype of stageRace table, it has 3 attributes: id(unique), name 
			and Number of stage named numberStage -->

		<itemtype code="StageRace" autocreate="true" generate="true">
			<deployment table="StageRace" typecode="15698" />
			<attributes>
				<attribute qualifier="id" type="java.lang.String">
					<modifiers optional="false" unique="true" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="name" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="numberStage" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
			</attributes>
		</itemtype>



		<!-- itemtype of Stage table, it has 3 attributes: id(unique), name and 
			stageLength -->
		<itemtype code="Stage" autocreate="true" generate="true">
			<deployment table="Stage" typecode="15458" />
			<attributes>
				<attribute qualifier="id" type="java.lang.String">
					<modifiers optional="false" unique="true" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="name" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="stageLength" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="date" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
			</attributes>
		</itemtype>



		<!-- itemtype of StageRanking table -->
		<itemtype code="StageRanking" autocreate="true" generate="true">
			<deployment table="StageRanking" typecode="15631" />
			<attributes>
				<attribute qualifier="id" type="java.lang.String">
					<modifiers optional="false" unique="true" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="time" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="position" type="java.lang.String">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
				<attribute qualifier="retired" type="retiredEnum">
					<modifiers optional="true" unique="false" />
					<persistence type="property">
					</persistence>
				</attribute>
			</attributes>
		</itemtype>

	</itemtypes>
</items>
